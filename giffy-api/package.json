{
  "name": "giffy-api",
  "version": "1.0.0",
  "description": "A simple enough node server scaffolding to get you started on your project immediately",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "_moduleAliases": {
    "@root": "server",
    "@core": "server/core",
    "@api": "server/api",
    "@config": "config/",
    "@middlewares": "server/middlewares"
  },
  "scripts": {
    "start": "set DEBUG=gif-verse-api & node index.js",
    "start-dev": "set DEBUG=gif-verse-api & nodemon index.js",
    "test": "mocha -r server/testHelpers/globals.js server/**/*.test.js",
    "lint": "eslint server/",
    "gen-resource": "node ./tools/cli.js"
  },
  "keywords": [
    "node",
    "scaffold",
    "expressjs",
    "server",
    "node",
    "project",
    "nodejs",
    "server",
    "simple",
    "http"
  ],
  "author": "Mohaimin",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.14",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "eslint": "7.19.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "handlebars": "^4.7.6",
    "mocha": "^8.2.1",
    "module-alias": "^2.2.2"
  }
}
